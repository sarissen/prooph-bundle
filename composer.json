{
    "name": "afs/prooph-bundle",
    "description": "Different integration of Prooph into Symfony",
    "type": "symfony-bundle",
    "license": "proprietary",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url":  "git@github.com:sarissen/bus-bundle.git",
            "vendor-alias": "afs"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "afs/bus-bundle": "^0.1",
        "prooph/common": "^4.2",
        "prooph/event-sourcing": "^5.3",
        "prooph/event-store": "^7.3",
        "prooph/event-store-symfony-bundle": "^0.4",
        "prooph/pdo-event-store": "^1.7@dev",
        "symfony/console": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/serializer-pack": "^1.0",
        "symfony/yaml": "^4.1",
        "doctrine/dbal": "^2.8@dev"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "AFS\\ProophBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
